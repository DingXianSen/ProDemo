<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--monthView-->
    <declare-styleable name="CalendarView">
        <attr name="cl_textColor" format="color" />
        <attr name="cl_monthPaddingLeft" format="dimension" />
        <attr name="cl_monthPaddingTop" format="dimension" />
        <attr name="cl_monthPaddingRight" format="dimension" />
        <attr name="cl_monthPaddingBottom" format="dimension" />
        <attr name="cl_cornerRadius" format="dimension" />
        <attr name="cl_selectTextColor" format="color" />
        <attr name="cl_weekendTextColor" format="color" />
        <attr name="cl_disTextColor" format="color" />
        <attr name="cl_sameTextColor" format="color" />
        <attr name="cl_topTextColor" format="color" />
        <attr name="cl_selectMaxRange" format="integer" />
        <attr name="cl_textSize" format="dimension" />
        <attr name="cl_textStyle" format="flags">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />>
        </attr>
        <attr name="cl_topTextSize" format="dimension" />
        <attr name="cl_bottomTextSize" format="dimension" />
        <attr name="cl_firstTopMargin" format="dimension" />
        <attr name="cl_secondTopMargin" format="dimension" />
        <attr name="cl_thirdTopMargin" format="dimension" />
        <attr name="cl_dividerHeight" format="dimension" />
        <attr name="cl_dividerColor" format="color" />
        <attr name="cl_firstSelectDayText" format="string" />
        <attr name="cl_lastSelectDayText" format="string" />
        <attr name="cl_rowHeight" format="dimension" />
        <attr name="cl_selectBgDrawable" format="reference" />
        <attr name="cl_selectRangeBgDrawable" format="reference" />
    </declare-styleable>


    <declare-styleable name="MonthLableView">
        <attr name="cl_lableWeekendTextColor" format="color" />
        <attr name="cl_lableTextColor" format="color" />
        <attr name="cl_lableTextSize" format="dimension" />
        <attr name="cl_lableArr" format="reference" />
    </declare-styleable>
</resources>